<h1>Listing type_rooms</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @type_rooms.each do |type_room| %>
  <tr>
    <td><%= type_room.name %></td>
    <td><%= link_to 'Show', type_room %></td>
    <td><%= link_to 'Edit', edit_type_room_path(type_room) %></td>
    <td><%= link_to 'Destroy', type_room, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

